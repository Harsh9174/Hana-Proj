FUNCTION "Animecode" (InputAnimecode NVARCHAR(10))
  RETURNS TABLE(
    "ANIMECODE" NVARCHAR(10),
	"RELEASEDDATE" DATE,
	"RELEASEDMONTH" NVARCHAR(10),
	"RELEASEDYEAR" INTEGER,
	"RATING" INTEGER
  )
  LANGUAGE SQLSCRIPT
  SQL SECURITY INVOKER AS
BEGIN
  RETURN
  SELECT *
  FROM "ANIMERELEASEDDETAILS"
  WHERE "ANIMECODE" = InputAnimecode;
END;






