COLUMN TABLE "Addresses" (
  "ADDRESSID" NVARCHAR(10) NOT NULL,
  "STREET" NVARCHAR(60),
  "CITY" VARCHAR(40),
  "POSTALCODE" NVARCHAR(10),
  "BUILDING" NVARCHAR(10),
  "COUNTRY" NVARCHAR(3),
  "REGION" NVARCHAR(4),
  "ADDRESSTYPE" NVARCHAR(2),
  "VALIDITY_STARTDATE" DATE ,
  "VALIDITY_
ENDDATE" DATE ,
  "LATITUDE" DOUBLE ,
  "LONGITUDE" DOUBLE ,
  PRIMARY KEY ("ADDRESSID")
)