COLUMN TABLE "Employees" (
  "EMPLOYEEID" NVARCHAR(10) NOT NULL,
  "NAME_FIRST" VARCHAR(40),
  "NAME_MIDDLE" VARCHAR(40),
  "NAME_LAST" VARCHAR(40),
  "NAME_INITIALS" NVARCHAR(10),
  "SEX" NVARCHAR(1),
  "LANGUAGE" NVARCHAR(2),
  "PHONENUMBER" NVARCHAR(30),
  "EMAILADDRESS" NVARCHAR(255),
  "LOGINNAME" NVARCHAR(12),
  "ADDRESSID" NVARCHAR(10),
  "VALIDITY_STARTDATE" DATE,
  "VALIDITY_ENDDATE" DATE,
  PRIMARY KEY ("EMPLOYEEID", "VALIDITY_ENDDATE"))